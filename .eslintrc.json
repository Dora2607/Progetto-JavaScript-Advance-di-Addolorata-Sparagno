{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "prettier/prettier": "error"
    }
}
